# Evaluate a postfix expression:

from typing import List

class Solution:
    def evalPE(self, tokens: List[str]) -> int:
        stack = []
        for t in tokens:
            if t in ['+','-','*','/']:
                b = stack.pop()
                a = stack.pop()
                if t == '+':
                    stack.append(a+b)
                elif t == '-':
                    stack.append(a-b)
                elif t == '*':
                    stack.append(a*b)
                elif t == '/':
                    stack.append(int(a/b))
            else:
                    stack.append(int(t))
                    
        return stack[0]
            
